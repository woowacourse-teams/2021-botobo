{
  "name": "botobo",
  "version": "1.2.0",
  "author": "Team Botobo",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "start": "cp ../dev/env/.env.local ./.env && webpack serve --node-env development --config webpack.dev.js",
    "build:local": "cp ../dev/env/.env.local ./.env && webpack --node-env production --config webpack.prod.js",
    "build:dev": "cp ../dev/env/.env.dev ./.env && webpack --node-env production --config webpack.prod.js",
    "build:prod": "cp ../dev/env/.env.prod ./.env && webpack --node-env production --config webpack.prod.js",
    "test": "jest",
    "build-server:local": "cp ../dev/env/.env.local ./.env && webpack --node-env production --config webpack.server.js",
    "build-server:dev": "cp ../dev/env/.env.dev ./.env && webpack --node-env production --config webpack.server.js",
    "build-server:prod": "cp ../dev/env/.env.prod ./.env && webpack --node-env production --config webpack.server.js",
    "start-server": "node ./dist-server/index.js",
    "build-concurrently:local": "concurrently \"yarn build:local\" \"yarn build-server:local\"",
    "build-concurrently:dev": "concurrently \"yarn build:dev\" \"yarn build-server:dev\"",
    "build-concurrently:prod": "concurrently \"yarn build:prod\" \"yarn build-server:prod\""
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "axios": "^0.21.1",
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "recoil": "^0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@emotion/babel-plugin": "^11.3.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "^3.15.2",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "fork-ts-checker-webpack-plugin": "^6.2.13",
    "html-webpack-plugin": "^5.3.2",
    "import-sort-style-module": "^6.0.0",
    "jest": "^27.0.6",
    "msw": "^0.33.2",
    "prettier": "^2.3.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "react-refresh": "^0.10.0",
    "regenerator-runtime": "^0.13.7",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "resolutions": {
    "chokidar": "^3.4.0",
    "xmldom": "github:xmldom/xmldom#0.7.0"
  },
  "msw": {
    "workerDirectory": "frontend"
  }
}
