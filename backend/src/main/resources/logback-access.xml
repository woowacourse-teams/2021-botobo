<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%n###### HTTP Request ######%n%fullRequest%n###### HTTP Response ######%n%fullResponse%n%n
            </pattern>
        </encoder>
    </appender>
    <appender-ref ref="CONSOLE"/>
</configuration>